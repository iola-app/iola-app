{
  "name": "ApolloMessenger",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prestart": "npm run schema:generate",
    "start": "concurrently -p name -n \"schemas,server\" \"npm run schema:watch\" \"npm run server:start\"",
    "server:start": "REACT_DEBUGGER=\"npm run debugger\" react-native start",
    "run:android": "react-native run-android",
    "test": "jest",
    "debugger": "rndebugger-open --open --port 8081",
    "schema:load": "graphql get-schema",
    "schema:watch": "babel-node ./scripts/watchSchemas.js",
    "schema:generate": "babel-node ./scripts/generateSchema.js"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.7",
    "apollo-client": "^2.2.3",
    "apollo-link": "^1.1.0",
    "apollo-link-batch-http": "^1.0.5",
    "apollo-link-http": "^1.3.3",
    "apollo-link-state": "^0.4.0",
    "babel-preset-react-native": "4.0.0",
    "graphql": "^0.13.0",
    "graphql-anywhere": "^4.1.3",
    "graphql-tag": "^2.7.3",
    "lodash": "^4.17.5",
    "native-base": "^2.3.8",
    "prop-types": "^15.6.0",
    "react": "16.2.0",
    "react-apollo": "^2.0.4",
    "react-native": "0.53.0",
    "react-navigation": "^1.0.3",
    "recompose": "^0.26.0",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "22.2.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "child-process-promise": "^2.2.1",
    "concurrently": "^3.5.1",
    "glob-watcher": "^5.0.1",
    "graphql-cli": "^2.14.3",
    "graphql-tools": "^2.21.0",
    "jest": "22.2.2",
    "merge-graphql-schemas": "^1.4.0",
    "react-native-debugger-open": "^0.3.17"
  },
  "jest": {
    "preset": "react-native"
  },
  "resolutions": {
    "graphql": "^0.13.0"
  }
}

    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-precise-range-plugin": "^1.3.0",
    "native-base": "^2.12.0",
    "node-libs-react-native": "^1.0.3",
    "npm-scripts-info": "^0.3.9",
    "promise-delay": "^2.1.0",
    "prop-types": "15.6.2",

{
  "name": "iola",
  "version": "1.0.4",
  "private": true,
  "scripts": {
    "info": "npm-scripts-info",
    "prestart": "yarn run schema:generate",
    "start": "yarn lint:statistics; concurrently -p name -n \"schemas,server\" \"yarn run schema:watch\" \"yarn run server:start\"",
    "server:start": "REACT_DEBUGGER=\"yarn run debugger\" react-native start --reset-cache",
    "run:android": "react-native run-android --variant=applicationDebug",
    "run:android-storybook": "yarn storybook:generate && react-native run-android --variant=storybookDebug --appIdSuffix=storybook",
    "log:android": "react-native log-android",
    "log:ios": "react-native log-ios",
    "run:ios": ". $PWD/.env && react-native run-ios --simulator \"$DEV_IOS_DEVICE\"",
    "run:ios-storybook": ". $PWD/.env && react-native run-ios --simulator \"$DEV_IOS_DEVICE\" --scheme Storybook --configuration Storybook",
    "test": "jest",
    "debugger": "rndebugger-open --open --port 8081",
    "schema:load": "graphql get-schema",
    "schema:watch": "babel-node $NODE_DEBUG_OPTION ./scripts/watchSchemas.js",
    "schema:generate": "yarn schema:generate-types-introspection && babel-node $NODE_DEBUG_OPTION ./scripts/generateSchema.js",
    "schema:generate-types-introspection": "babel-node $NODE_DEBUG_OPTION ./scripts/generateTypesIntrospection.js ./src/graph/meta/fragmentTypes.json",
    "storybook": "concurrently -p name -n \"stories,server\" \"yarn storybook:watch\" \"yarn storybook:server\"",
    "prestorybook": "yarn storybook:generate",
    "storybook:server": ". $PWD/.env && start-storybook -h $DEV_IP -c ./src/storybook",
    "storybook:generate": "rnstl",
    "storybook:watch": "babel-node $NODE_DEBUG_OPTION ./scripts/watchStories.js",
    "lint": "eslint \"./src/**/*.js\" --quiet; exit 0",
    "lint:statistics": "echo \"================\nESLint Statstics\n================\n\"; eslint -c .eslintrc ./src/**/*.js --format ./node_modules/eslint-stats/byErrorAndWarning.js; exit 0",
    "lint:fix": "eslint --fix \"./src/**/*.js\"; exit 0",
    "tools:generate-component": "generact ./src/components/ComponentTemplate/index.js --root ./src/components/",
    "postinstall": "sh ./scripts/postinstall.sh",
    "release:build:android": "cd android && ./gradlew bundleApplicationRelease",
    "release:test:android": "react-native run-android --variant=applicationRelease",
    "fastlane:ios:beta": "bundle exec fastlane ios beta"
  },
  "scripts-info": {
    "info": "Displays information about the scripts",
    "start": "Run the dev server",
    "run:android": "Build the Android app",
    "log:android": "Read the Android app logs",
    "run:ios": "Build the iOS app",
    "log:ios": "Read the iOS app logs",
    "storybook": "Run the Storybook server",
    "run:android-storybook": "Build the Storybook app for Android",
    "lint": "Display ESLint errors (without warnings)",
    "lint:statistics": "Display general ESLint Statistics",
    "lint:fix": "Automatically fix ESLint errors",
    "release:build:android": "Build the prod APK version",
    "release:test:android": "Build the prod APK version and install it on emulator/device"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./src/storybook/storyLoader.js"
    }
  },
  "xcodeSchemes": {
    "Debug": [
      "Storybook"
    ],
    "Release": [],
    "projectDirectory": "ios",
    "settings": {}
  },

{
  "name": "ApolloMessenger",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "info": "npm-scripts-info",
    "prestart": "yarn run schema:generate",
    "start": "concurrently -p name -n \"schemas,server\" \"yarn run schema:watch\" \"yarn run server:start\"",
    "server:start": "REACT_DEBUGGER=\"yarn run debugger\" react-native start --reset-cache --config $PWD/rn-cli.config.js",
    "run:android": "react-native run-android --variant=mainDebug",
    "run:android-storybook": "yarn storybook:generate && react-native run-android --variant=storybookDebug --appIdSuffix=storybook",
    "log:android": "react-native log-android",
    "run:ios": "react-native run-ios",
    "test": "jest",
    "debugger": "rndebugger-open --open --port 8081",
    "schema:load": "graphql get-schema",
    "schema:watch": "babel-node $NODE_DEBUG_OPTION ./scripts/watchSchemas.js",
    "schema:generate": "yarn schema:generate-types-introspection && babel-node $NODE_DEBUG_OPTION ./scripts/generateSchema.js",
    "schema:generate-types-introspection": "babel-node $NODE_DEBUG_OPTION ./scripts/generateTypesIntrospection.js ./src/graph/meta/fragmentTypes.json",
    "storybook": "concurrently -p name -n \"stories,server\" \"yarn storybook:watch\" \"yarn storybook:server\"",
    "prestorybook": "yarn storybook:generate",
    "storybook:server": "storybook start -p 7007 -c ./src/storybook --skip-packager",
    "storybook:generate": "rnstl",
    "storybook:watch": "babel-node $NODE_DEBUG_OPTION ./scripts/watchStories.js",
    "tools:generate-component": "generact ./src/components/ComponentTemplate/index.js --root ./src/components/",
    "postinstall": "sh ./scripts/postinstall.sh"
  },
  "scripts-info": {
    "info": "Displays information about the scripts",
    "start": "Run the dev server",
    "log:android": "Read the Android app logs",
    "run:android": "Build the Android app",
    "log:ios": "Read the iOS app logs",
    "run:ios": "Build the iOS app",
    "storybook": "Run the Storybook server",
    "run:android-storybook": "Build the Storybook app for Android"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./src/storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.6",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.3.7",
    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "apollo-upload-client": "^8.1.0",
    "apollo-utilities": "^1.0.17",
    "babel-eslint": "^10.0.1",
    "buffer": "^5.2.1",
    "eslint-plugin-react-native": "^3.5.0",
    "formik": "1.3.1",
    "graphql": "^0.13.2",
    "graphql-anywhere": "^4.1.15",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "3.1.1",
    "hoist-non-react-statics": "^3.1.0",
    "http-header-fields": "^1.0.0",
    "immutability-helper": "^2.8.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "moment-precise-range-plugin": "^1.3.0",
    "native-base": "^2.8.1",

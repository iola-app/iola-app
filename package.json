{
  "name": "ApolloMessenger",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prestart": "yarn run schema:generate",
    "start": "concurrently -p name -n \"schemas,server\" \"yarn run schema:watch\" \"yarn run server:start\"",
    "server:start": "REACT_DEBUGGER=\"yarn run debugger\" react-native start -- --reset-cache",
    "run:android": "react-native run-android",
    "log:android": "react-native log-android",
    "run:ios": "react-native run-ios",
    "test": "jest",
    "debugger": "rndebugger-open --open --port 8081",
    "schema:load": "graphql get-schema",
    "schema:watch": "babel-node $NODE_DEBUG_OPTION ./scripts/watchSchemas.js",
    "schema:generate": "babel-node $NODE_DEBUG_OPTION ./scripts/generateSchema.js"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.10",
    "apollo-client": "^2.2.6",
    "apollo-link": "^1.2.1",
    "apollo-link-batch-http": "^1.2.1",
    "apollo-link-context": "^1.0.7",
    "apollo-link-http": "^1.5.3",
    "apollo-link-state": "^0.4.0",
    "apollo-utilities": "^1.0.11",
    "babel-preset-react-native": "4.0.0",
    "formik": "^0.11.11",
    "graphql": "^0.13.1",
    "graphql-anywhere": "^4.1.8",
    "graphql-tag": "^2.8.0",
    "hoist-non-react-statics": "^2.5.0",
    "http-header-fields": "^1.0.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "native-base": "^2.3.10",
    "native-base-shoutem-theme": "github:greyexpert/theme#master",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-moment": "^0.7.0",
    "react-native": "0.53.0",
    "react-native-animatable": "^1.2.4",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-splash-screen": "^3.0.6",
    "react-navigation": "^1.5.1",
    "recompose": "^0.26.0",
    "yup": "^0.24.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "22.4.1",
    "babel-plugin-inline-import-graphql-ast": "^2.3.3",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "child-process-promise": "^2.2.1",
    "concurrently": "^3.5.1",
    "glob-watcher": "^5.0.1",
    "graphql-cli": "^2.15.8",
    "graphql-tools": "^2.21.0",
    "jest": "22.4.2",
    "merge-graphql-schemas": "^1.5.0",
    "react-native-debugger-open": "^0.3.17"
  },
  "jest": {
    "preset": "react-native"
  },
  "resolutions": {
    "graphql": "^0.13.1",
    "native-base-shoutem-theme": "github:greyexpert/theme#master"
  }
}

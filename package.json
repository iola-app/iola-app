{
  "name": "ApolloMessenger",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prestart": "yarn run schema:generate",
    "start": "concurrently -p name -n \"schemas,server\" \"yarn run schema:watch\" \"yarn run server:start\"",
    "server:start": "REACT_DEBUGGER=\"yarn run debugger\" react-native start --reset-cache --config $PWD/rn-cli.config.js",
    "run:android": "react-native run-android --variant=mainDebug",
    "run:android-storybook": "yarn storybook:generate && react-native run-android --variant=storybookDebug --appIdSuffix=storybook",
    "log:android": "react-native log-android",
    "run:ios": "react-native run-ios",
    "test": "jest",
    "debugger": "rndebugger-open --open --port 8081",
    "schema:load": "graphql get-schema",
    "schema:watch": "babel-node $NODE_DEBUG_OPTION ./scripts/watchSchemas.js",
    "schema:generate": "yarn schema:generate-types-introspection && babel-node $NODE_DEBUG_OPTION ./scripts/generateSchema.js",
    "schema:generate-types-introspection": "babel-node $NODE_DEBUG_OPTION ./scripts/generateTypesIntrospection.js ./src/graph/meta/fragmentTypes.json",
    "storybook": "concurrently -p name -n \"stories,server\" \"yarn storybook:watch\" \"yarn storybook:server\"",
    "prestorybook": "yarn storybook:generate",
    "storybook:server": "storybook start -p 7007 -c ./src/storybook --skip-packager",
    "storybook:generate": "rnstl",
    "storybook:watch": "babel-node $NODE_DEBUG_OPTION ./scripts/watchStories.js",
    "tools:generate-component": "generact ./src/components/ComponentTemplate/index.js --root ./src/components/",
    "postinstall": "sh ./scripts/postinstall.sh"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./src/storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.2",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.3.2",
    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "apollo-upload-client": "^8.0.0",
    "apollo-utilities": "^1.0.13",
    "babel-eslint": "^8.2.3",
    "buffer": "^5.2.0",
    "eslint-plugin-react-native": "^3.2.1",
    "formik": "1.0.2",
    "graphql": "^0.13.2",
    "graphql-anywhere": "^4.1.11",
    "graphql-tag": "^2.9.2",
    "hoist-non-react-statics": "^3.0.1",
    "http-header-fields": "^1.0.0",
    "immutability-helper": "^2.7.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "moment-precise-range-plugin": "^1.3.0",
    "native-base": "^2.5.1",
    "native-base-shoutem-theme": "github:greyexpert/theme#master",
    "node-libs-react-native": "^1.0.2",
    "promise-delay": "^2.1.0",
    "prop-types": "^15.6.2",
    "react": "16.3.2",
    "react-apollo": "^2.1.4",
    "react-gateway": "^3.0.0",
    "react-moment": "^0.7.0",
    "react-native": "0.55.4",
    "react-native-actionsheet": "^2.4.2",
    "react-native-animatable": "^1.3.0",
    "react-native-confirmation-code-input": "^1.0.4",
    "react-native-device-info": "^0.22.2",
    "react-native-event-source": "^1.0.1",
    "react-native-extra-dimensions-android": "^0.21.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-image-crop-picker": "^0.20.3",
    "react-native-image-zoom-viewer": "^2.2.5",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-modal": "^6.0.0",
    "react-native-on-layout": "^2.0.2",
    "react-native-progress": "^3.5.0",
    "react-native-splash-screen": "3.0.6",
    "react-native-wheel-picker-android": "^1.1.2",
    "react-navigation": "^2.3.1",
    "react-navigation-tabs": "^0.5.1",
    "recompose": "^0.28.1",
    "subscriptions-transport-sse": "^0.9.2",
    "url-parse": "^1.4.3",
    "uuid": "^3.3.2",
    "yup": "^0.26.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.2",
    "@storybook/addon-console": "^1.0.4",
    "@storybook/addon-knobs": "^3.4.7",
    "@storybook/addon-links": "^4.0.0-alpha.2",
    "@storybook/addons": "^4.0.0-alpha.2",
    "@storybook/react-native": "^4.0.0-alpha.2",
    "apollo-link-schema": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23.0.1",
    "babel-plugin-import-graphql": "^2.5.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-runtime": "^7.0.0-beta.3",
    "child-process-promise": "^2.2.1",
    "concurrently": "^3.6.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "faker": "^4.1.0",
    "generact": "^0.3.2",
    "glob-watcher": "^5.0.1",
    "graphql-cli": "^2.16.3",
    "graphql-relay": "^0.5.5",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^3.0.2",
    "jest": "23.1.0",
    "merge-graphql-schemas": "^1.5.1",
    "mock-async-storage": "^2.0.3",
    "node-fetch": "^2.2.0",
    "react-dom": "^16.3.1",
    "react-native-debugger-open": "^0.3.17",
    "react-native-storybook-loader": "^1.8.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "resolutions": {
    "graphql": "^0.13.2",
    "native-base-shoutem-theme": "github:greyexpert/theme#master"
  }
}
